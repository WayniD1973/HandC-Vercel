function cycleCarousel(){document.querySelector(".carouselBarItemCircleSelected")==document.querySelectorAll(".carouselBarItemCircle")[0]?(document.querySelectorAll(".carouselBarItemCircle")[0].classList.remove(".carouselBarItemCircleSelected"),document.querySelectorAll(".carouselBarItemCircle")[1].classList.add(".carouselBarItemCircleSelected")):document.querySelector(".carouselBarItemCircleSelected")==document.querySelectorAll(".carouselBarItemCircle")[1]?(document.querySelectorAll(".carouselBarItemCircle")[1].classList.remove(".carouselBarItemCircleSelected"),document.querySelectorAll(".carouselBarItemCircle")[2].classList.add(".carouselBarItemCircleSelected")):document.querySelector(".carouselBarItemCircleSelected")==document.querySelectorAll(".carouselBarItemCircle")[2]&&(document.querySelectorAll(".carouselBarItemCircle")[2].classList.remove(".carouselBarItemCircleSelected"),document.querySelectorAll(".carouselBarItemCircle")[0].classList.add(".carouselBarItemCircleSelected"))}