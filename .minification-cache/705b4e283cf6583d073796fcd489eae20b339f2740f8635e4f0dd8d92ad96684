function accessCookie(e){for(var o=e+"=",n=document.cookie.split(";"),t=0;t<n.length;t++){var i=n[t].trim();if(0==i.indexOf(o))return i.substring(o.length,i.length)}return""}function createCookie(e,o,n){var t=new Date;t.setTime(t.getTime()+24*n*60*60*1e3),document.cookie=e+"="+o+"; expires="+t.toGMTString()+";path=/"}fetch("https://login-finished.wumpus-dev.repl.co/relogin",{method:"GET",headers:{Accept:"application/json",authorization:"Bearer "+accessCookie("connectionToken")}}).then(e=>e.json()).then(e=>createCookie("connectionToken",e.access_token,30));