var id;function submit(){localStorage.setItem("hammychatCustomChannelUse",document.getElementById("channelID").value),window.location.reload()}null==localStorage.getItem("hammychatCustomChannelUse")&&null==localStorage.getItem("hammychatCustomChannelUse")||(id=localStorage.getItem("hammychatCustomChannelUse"),localStorage.removeItem("hammychatCustomChannelUse"),window.location.replace("../../?channel=custom&id="+id));