function login(){fetch("https://h-and-c.co.uk/api/login",{method:"POST",headers:{"Content-Type":"application/json"},body:JSON.stringify({username:document.getElementById("usernameInput").value,password:document.getElementById("passwordInput").value})}).then(e=>e.json()).then(e=>{createCookie("connectionToken",e.access_token,30);e=new URLSearchParams(window.location.search);e.get("redirect")?window.location.replace(e.get("redirect")):window.location.replace("/")})}function useOtherDevice(){console.log(scripManager.onlineManager.io)}