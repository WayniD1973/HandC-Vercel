var selectedItem=2;function cycleCarousel(e){0==e?--selectedItem:selectedItem+=1}function changeCarouselItem(){0==selectedItem?(document.querySelectorAll(".carouselBarItemCircle").forEach(e=>{e.classList.contains("carouselBarItemCircleSelected")&&e.classList.remove("carouselBarItemCircleSelected")}),document.querySelectorAll(".carouselBarItemCircle")[0].classList.add("carouselBarItemCircleSelected"),selectedItem=1):1==selectedItem?(document.querySelectorAll(".carouselBarItemCircle").forEach(e=>{e.classList.contains("carouselBarItemCircleSelected")&&e.classList.remove("carouselBarItemCircleSelected")}),document.querySelectorAll(".carouselBarItemCircle")[1].classList.add("carouselBarItemCircleSelected"),selectedItem=2):2==selectedItem?(document.querySelectorAll(".carouselBarItemCircle").forEach(e=>{e.classList.contains("carouselBarItemCircleSelected")&&e.classList.remove("carouselBarItemCircleSelected")}),document.querySelectorAll(".carouselBarItemCircle")[2].classList.add("carouselBarItemCircleSelected"),selectedItem=3):3==selectedItem?(document.querySelectorAll(".carouselBarItemCircle").forEach(e=>{e.classList.contains("carouselBarItemCircleSelected")&&e.classList.remove("carouselBarItemCircleSelected")}),document.querySelectorAll(".carouselBarItemCircle")[0].classList.add("carouselBarItemCircleSelected"),selectedItem=1):4==selectedItem&&(document.querySelectorAll(".carouselBarItemCircle").forEach(e=>{e.classList.contains("carouselBarItemCircleSelected")&&e.classList.remove("carouselBarItemCircleSelected")}),document.querySelectorAll(".carouselBarItemCircle")[0].classList.add("carouselBarItemCircleSelected"),selectedItem=1)}