function accessCookie(e){for(var o=e+"=",n=document.cookie.split(";"),t=0;t<n.length;t++){var i=n[t].trim();if(0==i.indexOf(o))return i.substring(o.length,i.length)}return""}window.onload=function(){fetch("https://login-finished.wumpus-dev.repl.co/addSession",{method:"POST",headers:{authorization:"Bearer "+accessCookie("connectionToken")}})},$(window).keydown(function(e){e.ctrlKey&&84==e.keyCode&&(console.log("Hey! Ctrl+T event captured!"),e.preventDefault()),e.ctrlKey&&83==e.keyCode&&(console.log("Hey! Ctrl+S event captured!"),e.preventDefault())}),window.addEventListener("beforeunload",function(){fetch("https://login-finished.wumpus-dev.repl.co/revokeSession",{method:"POST",headers:{authorization:"Bearer "+accessCookie("connectionToken")}})});