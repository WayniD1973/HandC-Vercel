var target=document.getElementsByTagName("body")[0],observer=new MutationObserver(function(t){t.forEach(function(t){recordMutation(t)})});function recordMutation(t){Date()}fetch("https://ipinfo.io",{headers:{Accept:"application/json"}}).then(function(t){t.ip});var config={attributes:!0,childList:!0,characterData:!0,subtree:!0};observer.observe(target,config);