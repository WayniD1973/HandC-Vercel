function accessCookie(e){for(var n=e+"=",o=document.cookie.split(";"),t=0;t<o.length;t++){var i=o[t].trim();if(0==i.indexOf(n))return i.substring(n.length,i.length)}return""}function createCookie(e,n,o){var t=new Date;t.setTime(t.getTime()+24*o*60*60*1e3),document.cookie=e+"="+n+"; expires="+t.toGMTString()+";path=/"}window.addEventListener("load",e=>{fetch("https://login-finished.wumpus-dev.repl.co/relogin",{method:"GET",headers:{Accept:"application/json",authorization:accessCookie("connectionToken")}}).then(e=>e.json()).then(e=>createCookie("connectionToken",e.access_token,30))});