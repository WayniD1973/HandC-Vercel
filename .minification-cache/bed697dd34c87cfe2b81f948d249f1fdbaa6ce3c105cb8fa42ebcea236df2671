setTimeout(function(){var e=!0;document.querySelector(".searchBoxInput").addEventListener("keyup",()=>{""==document.querySelector(".searchBoxInput").value?document.querySelector(".searchBoxButton").innerText="Cancel":document.querySelector(".searchBoxButton").innerText="Search"}),document.querySelector(".navItem-search").addEventListener("click",()=>{document.querySelector(".navItems-left").style.display="none",document.querySelector(".navItems-right").style.display="none",document.querySelector(".searchBox").style.display="flex"}),document.querySelector(".searchBoxButton").addEventListener("click",()=>{document.querySelector(".navItems-left").style.display="flex",document.querySelector(".navItems-right").style.display="flex",document.querySelector(".searchBox").style.display="none"}),document.querySelector(".navbarTwo hc-menuButton").addEventListener("click",()=>{e=1==e?(document.querySelector(".sidebar").classList.add("openSidebar"),!1):(document.querySelector(".sidebar").classList.remove("openSidebar"),!0)})},500);