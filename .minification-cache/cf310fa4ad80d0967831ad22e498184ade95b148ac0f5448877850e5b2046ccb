var observeDOM=function(){var t=window.MutationObserver||window.WebKitMutationObserver;return function(e,n){var r;if(e&&1===e.nodeType)return t?((r=new t(n)).observe(e,{childList:!0,subtree:!0}),r):void(window.addEventListener&&(e.addEventListener("DOMNodeInserted",n,!1),e.addEventListener("DOMNodeRemoved",n,!1)))}}();