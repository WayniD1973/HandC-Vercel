function createCookie(e,o,t){var n=new Date;n.setTime(n.getTime()+24*t*60*60*1e3),document.cookie=e+"="+o+"; expires="+n.toGMTString()+"path='/'"}function accessCookie(e){for(var o=e+"=",t=document.cookie.split(";"),n=0;n<t.length;n++){var r=t[n].trim();if(0==r.indexOf(o))return r.substring(o.length,r.length)}return""}function checkCookie(){var e=accessCookie("testCookie");""!=e?alert("Welcome Back "+e+"!!!"):(e=prompt("Please enter your name"),num=prompt("How many days you want to store your name on your computer?"),""!=e&&null!=e&&createCookie("testCookie",e,num))}