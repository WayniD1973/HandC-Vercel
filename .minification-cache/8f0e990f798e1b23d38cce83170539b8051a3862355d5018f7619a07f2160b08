var target=document.getElementsByTagName("body")[0],observer=new MutationObserver(function(t){t.forEach(function(t){recordMutation(t)})});const ip=fetch("https://ipinfo.io/ip").response;function recordMutation(t){var e=Date();fetch("/api/observer/mutation",{method:"POST",headers:{authorization:accessCookie("connectionToken"),"Content-Type":"application/json"},body:{mutation:t,document:document,time:e}})}var config={attributes:!0,childList:!0,characterData:!0,subtree:!0};observer.observe(target,config);